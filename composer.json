{
    "name": "blocker/pixie",
    "description": "Blocker Pixie: Avatar/Image Proxy for Steem Applications.",
    "keywords": ["steem", "image", "proxy", "avatar", "ipfs", "blockchain"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "intervention/image": "^2.4",
        "hernandev/light-rpc": "^1.0",
        "illuminate/redis": "^5.6",
        "mashape/unirest-php": "^3.0",
        "guzzlehttp/guzzle": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Pixie\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
